<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dgut.music_online.dao.SongListDao">
    <insert id="insertSongList" parameterType="com.dgut.music_online.domain.SongList">
        insert into music_online.song
        values
        (#{id}, #{creatorId}, #{description}, #{name}, #{coverImgUrl})
    </insert>
</mapper>